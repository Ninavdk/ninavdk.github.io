---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Ninavdk's Blog</title>
	<link href="http://ninavdk.com/blog/atom.xml" rel="self"/>
	<link href="http://ninavdk.com/blog/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://ninavdk.com/blog/</id>
	<author>
		<name>Ninavdk</name>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://ninavdk.com{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://hankquinlan.github.io/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
